<h1>vehicle</h1>


      
      <table class="table table-hover table-info table-bordered border-dark">
        <thead class="table-dark">
          <tr>
            <th>Model</th>
            <th>Number</th>
            <th>Profile</th>
            <th colspan="3"></th> 
          </tr>
        </thead>
        <tbody>
          <% @vehicles.each do |vehicle| %>
          <tr>
            <td><%=vehicle.vehicle_model%></td>
            <td><%=vehicle.vehicle_number%></td>
            <td></td>
            <td><%= link_to 'Show', vehicle, class: "btn btn-primary" %></td>
            <td><%= link_to 'Edit', vehicle, class: "btn btn-primary" %></td>
            <td><%= link_to 'Destroy', vehicle, class: "btn btn-primary" %></td>
          </tr>


  <% end %>
  </tbody>


