<%= form_with model: [ @vehicle, @fastag ], url: vehicle_fastags_path(@vehicle) do |form| %>
  <p>
    <%= form.label :accountnumber %><br>
    <%= form.text_field :accountnumber %>
  </p>
  <p>
    <%= form.label :vehiclenumber %><br>
    <%= form.text_field :vehiclenumber %>
  </p>
  <p>
    <%= form.hidden_field :vehicle_id %>
  </p>
  <p>
    <%= form.submit %>
  </p>
<% end %>
